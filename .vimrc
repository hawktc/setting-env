set nocompatible	" default editor
set background=dark	" dark background

syntax on
set mouse=a
set viewoptions=folds,options,cursor,unix,slash
set virtualedit=onemore
set history=100
" set spell

set cursorline
set backspace=indent,eol,start
" set nu
set showmatch
set incsearch
set hlsearch
set scrolloff=3

set nowrap
set autoindent
set shiftwidth=2
set expandtab
set tabstop=2
set softtabstop=2
" set pastetoggle=<F12>

" autocmd Filetype java,js,python,xml,shell

" color solarized

